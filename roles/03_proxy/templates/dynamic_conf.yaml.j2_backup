http:
  services:
    k3s-service:
      loadBalancer:
        servers:
          - url: "http://{{ k3s_tailscale_ip }}:80"
  middlewares:
    test-compress:
      compress: {}
  routers:
    k3s:
      rule: "HostRegexp(`{subdomain:[a-zA-Z0-9-]+}.tomik.lat`) || Host(`tomik.lat`)"
      service: k3s-service
      entryPoints:
        - websecure
      middlewares:
        - test-compress
      tls:
        certResolver: myresolver